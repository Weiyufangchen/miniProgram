/* pages/index/index.wxss */
page{
  height: 100%;
  background-color: #8ED145;
}
.indexContainer{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.indexImg{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx; 
  margin: 100rpx 0;
}
button{
  font-size: 32rpx;
  background-color: lightgreen;
}
.username{
  font-size: 32rpx;
  font-weight: bold;
  margin: 100rpx 0;
}
.goStudy{
  border: 1rpx solid #999;
  width: 300rpx;
  height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
}
