/* pages/template/list-template.wxss */
.tempContainer{
  display: flex;
  flex-direction: column;
  margin: 20rpx 0;
  /* border-top: 1rpx solid #aaa; */
  border-bottom: 1rpx solid #aaa;
  background-color: #fff;
}
.avatar_date{
  padding: 10rpx;
}
.avatar_date image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.avatar_date text{
  font-size: 32rpx;
}

.company{
  font-size: 36rpx;
  font-weight: 700;
  margin: 10rpx;
}
.contentImg{
  width: 100%;
  height: 460rpx;
}
.content{
  font-size: 32rpx;
  text-indent: 64rpx;
}
.star_view image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin: 10rpx;
}
.star_view text{
  font-size: 28rpx;
}
